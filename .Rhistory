uniproot = functin(f,lb,ub,d){
k=round((ub-lb)/d)
if(f(lb)*f(ub)<0) uniroot(f,lb,ub)
else{
for (i in 1:k) {
ubb=ub-i*d
if(f(lb)*f(ubb)<0) {cat('Up=',ubb,'f(lb)f(ub)<0');uniroot(f,lb,ubb);break}
}
}
}
